   3.775305661609592*^9}, {3.775308674794537*^9, 3.7753086960930557`*^9}, {
   3.775309566322361*^9, 3.775309566822324*^9}, 3.775541341784831*^9},
 CellLabel->"In[10]:=",ExpressionUUID->"cfc2f1e9-24e6-485a-a366-fd9472768ef0"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"partialCM", "[", 
    RowBox[{
    "N_", ",", "m_", ",", "l_", ",", "d_", ",", "dim1_", ",", "dim2_"}], 
    "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "dim", ",", " ", "halfdim", ",", " ", "bigCM", ",", " ", "indices1", 
       ",", " ", "indices2", ",", "indices", ",", " ", "partialCM", ",", " ", 
       "Tr"}], "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"bigCM", "=", 
       RowBox[{"CM", "[", 
        RowBox[{"N", ",", "m", ",", "l"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"dim", "=", 
       RowBox[{"Length", "[", "bigCM", "]"}]}], ";", " ", 
      RowBox[{"halfdim", "=", 
       RowBox[{"dim", "/", "2"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"indices1", "=", 
       RowBox[{
        RowBox[{"Thread", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Table", "[", 
            RowBox[{"x", ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "dim1"}], "}"}]}], "]"}], ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"x", "+", "halfdim"}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "dim1"}], "}"}]}], "]"}]}], "}"}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"indices2", "=", 
       RowBox[{
        RowBox[{"Thread", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"dim1", "+", "d", "+", "x"}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "dim2"}], "}"}]}], "]"}], ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"dim1", "+", "d", "+", "x", "+", "halfdim"}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "dim2"}], "}"}]}], "]"}]}], "}"}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"indices", "=", 
       RowBox[{"Join", "[", 
        RowBox[{"indices1", ",", "indices2"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"partialCM", "=", 
       RowBox[{"bigCM", "[", 
        RowBox[{"[", 
         RowBox[{"indices", ",", "indices"}], "]"}], "]"}]}]}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.77530567460966*^9, 3.7753057474846334`*^9}, {
