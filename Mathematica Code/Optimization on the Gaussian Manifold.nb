        RowBox[{"4", "/", 
         RowBox[{"l", "^", "2"}]}], "  ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Sin", "[", 
           RowBox[{"Pi", " ", 
            RowBox[{"k", "/", "N"}]}], "]"}], ")"}], "^", "2"}]}]}], "]"}]}], 
    "]"}]}], ";"}]}], "Input",
