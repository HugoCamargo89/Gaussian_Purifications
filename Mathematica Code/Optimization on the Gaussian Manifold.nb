 Background->GrayLevel[
  0.85],ExpressionUUID->"1be94dd6-a107-4b72-bf0d-9523361dd2d5"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FourierMatrixElement", "[", "N_", "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"k", ",", "x"}], "}"}], ",", 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"I", " ", "2", " ", "Pi", " ", "x", " ", 
        RowBox[{"k", " ", "/", "N"}]}], "]"}], "/", 
      RowBox[{"Sqrt", "[", "N", "]"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DispersionRelation", "[", 
    RowBox[{"m_", ",", "N_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", "k", "}"}], ",", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"m", "^", "2"}], "+", 
       RowBox[{
        RowBox[{"4", "/", 
         RowBox[{"l", "^", "2"}]}], "  ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Sin", "[", 
           RowBox[{"Pi", " ", 
            RowBox[{"k", "/", "N"}]}], "]"}], ")"}], "^", "2"}]}]}], "]"}]}], 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.775303771446594*^9, 3.775303847540307*^9}, {
   3.7753044069466248`*^9, 3.77530442747785*^9}, {3.775304555993222*^9, 
   3.7753045582432203`*^9}, 3.775541349319888*^9},
 CellLabel->"In[5]:=",ExpressionUUID->"f9bf936a-418c-44aa-a4a6-1237d5fd2152"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"FT", "[", "n_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"f", ",", "pos", ",", "val"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"f", "=", 
       RowBox[{"FourierMatrixElement", "[", "n", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"pos", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "k"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "n"}], "}"}]}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"val", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"f", "[", 
            RowBox[{"k", ",", "x"}], "]"}], "//", "N"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "n"}], "}"}]}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"SparseArray", "[", 
       RowBox[{"pos", "\[Rule]", "val"}], "]"}]}]}], "]"}]}], ";"}]], "Input",\

 CellChangeTimes->{{3.7753044394466305`*^9, 3.7753044400716295`*^9}, {
   3.7753044782591524`*^9, 3.77530462700885*^9}, {3.7753048011807213`*^9, 
   3.7753048380244474`*^9}, 3.7755413472470903`*^9},
 CellLabel->"In[7]:=",ExpressionUUID->"e559be64-ad25-48e9-b3e1-af6850e238e1"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Diag", "[", 
    RowBox[{"m_", ",", "n_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "\[Omega]", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Omega]", "=", 
       RowBox[{"DispersionRelation", "[", 
        RowBox[{"m", ",", "n", ",", "l"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"DiagonalMatrix", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"\[Omega]", "[", "k", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "n"}], "}"}]}], "]"}], "]"}]}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.775304874212018*^9, 3.775304991446345*^9}, {
   3.775308662890841*^9, 3.77530866379296*^9}, 3.775541345280986*^9},
 CellLabel->"In[8]:=",ExpressionUUID->"2f67d1cb-5207-40e9-b894-53dd42754394"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"invDiag", "[", 
    RowBox[{"m_", ",", "n_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "\[Omega]", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Omega]", "=", 
       RowBox[{"DispersionRelation", "[", 
        RowBox[{"m", ",", "n", ",", "l"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"DiagonalMatrix", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"1", "/", 
          RowBox[{"\[Omega]", "[", "k", "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "n"}], "}"}]}], "]"}], "]"}]}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.775308666234054*^9, 3.7753086692650213`*^9}, {
  3.775541342893167*^9, 3.775541343818965*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"17502578-9034-4c01-8cb3-b41fdf9ebd53"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"CM", "[", 
    RowBox[{"N_", ",", "m_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"F", ",", "D", ",", "invD", ",", "G", ",", "Tr"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"F", "=", 
       RowBox[{"FT", "[", "N", "]"}]}], ";", 
      RowBox[{"D", "=", 
       RowBox[{"Diag", "[", 
        RowBox[{"m", ",", "N", ",", "l"}], "]"}]}], ";", " ", 
      RowBox[{"invD", "=", 
       RowBox[{"invDiag", "[", 
        RowBox[{"m", ",", "N", ",", "l"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"G", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"ArrayFlatten", "[", 
          RowBox[{"(", "\[NoBreak]", GridBox[{
             {
              RowBox[{"F", ".", "D", ".", 
               RowBox[{"ConjugateTranspose", "[", "F", "]"}]}], "0"},
             {"0", 
              RowBox[{"F", ".", "invD", ".", 
               RowBox[{"ConjugateTranspose", "[", "F", "]"}]}]}
            }], "\[NoBreak]", ")"}], "]"}], "//", "Chop"}], "//", 
        "SparseArray"}]}]}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.775305048883794*^9, 3.77530504957135*^9}, {
   3.775305085305721*^9, 3.775305090774472*^9}, {3.7753051397385497`*^9, 
   3.7753052468791714`*^9}, {3.775305361265933*^9, 3.7753053619533815`*^9}, {
   3.7753054077815337`*^9, 3.775305447031464*^9}, {3.775305539797154*^9, 
