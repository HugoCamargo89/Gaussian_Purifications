Cell[TextData[StyleBox["2. Generate the purified state complex structure (in \
this case for a minimal purification)",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.775541691548297*^9, 3.7755417065181093`*^9}, {
  3.775541828555531*^9, 3.775541831328326*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"60cb2935-0c33-442d-bcb5-7e612bf3b320"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"rlist", ",", "MTra"}], "}"}], "=", 
   RowBox[{"GOExtractStdFormG", "[", "CM0", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"J0", "=", 
   RowBox[{"GOPurifyStandardJBoson", "[", 
    RowBox[{"rlist", ",", "6", ",", "\"\<qpqp\>\""}], "]"}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.775541682190981*^9, 3.77554177782376*^9}, {
  3.7756584797572603`*^9, 3.775658491461603*^9}, {3.7756586394444933`*^9, 
  3.775658663320425*^9}, {3.775658803123712*^9, 3.775658805980338*^9}, {
  3.775658899244485*^9, 3.7756589019972677`*^9}},
 CellLabel->"In[86]:=",ExpressionUUID->"59182147-27a2-4d85-bd25-d97741096658"],

Cell[TextData[StyleBox["3. Generate a list of the starting points for the \
optimization (in this case 50 random transformations)",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.775541798902584*^9, 3.775541841904106*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"34ec6c9e-c65b-4201-8dc6-f5ea22aef4bb"],

Cell[BoxData[
 RowBox[{
  RowBox[{"M0List", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"ArrayFlatten", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Inverse", "[", "MTra", "]"}], ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", 
           RowBox[{"GOMranSp", "[", 
            RowBox[{"12", ",", "\"\<qpqp\>\""}], "]"}]}], "}"}]}], "}"}], 
       "]"}], "//", "SparseArray"}], ",", 
     RowBox[{"{", "50", "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.775541849659087*^9, 3.775541885906084*^9}, {
  3.7756585145667753`*^9, 3.775658515965473*^9}},
 CellLabel->"In[95]:=",ExpressionUUID->"29f7bd6d-26a1-4eac-8e2d-c6cc013ea512"],

Cell[TextData[StyleBox["4. Define the function and gradient for the \
optimization (in this case choosing an equal distribution of deg. of freedom \
in the ancillary)",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.7755419413657427`*^9, 3.775541981278227*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"0da37d7e-0068-41a8-9d5f-461a1677db50"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Restriction", "=", 
   RowBox[{"GORestrictionAA", "[", 
    RowBox[{"{", 
     RowBox[{"3", ",", "3", ",", "3", ",", "3"}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"function", "=", 
   RowBox[{"GOEoPBos", "[", "Restriction", "]"}]}], ";", " ", 
  RowBox[{"gradient", "=", 
   RowBox[{"GOEoPgradBos", "[", "Restriction", "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.775541985290533*^9, 3.77554201468399*^9}},
 CellLabel->
  "In[104]:=",ExpressionUUID->"fd9d772e-e98f-4069-8000-5ed1fdbed01e"],

Cell[TextData[StyleBox["5. Execute the optimization",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.775541909217443*^9, 3.775541935738533*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"efef6185-8ab3-40c6-8c68-9db01f8c62e4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Result", "=", 
   RowBox[{"GOOptimize", "[", 
    RowBox[{"function", ",", "gradient", ",", "J0", ",", "M0List", ",", 
     RowBox[{"{", 
      RowBox[{"\"\<None\>\"", ",", "\"\<GOSpBasis\>\""}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"6", ",", "6"}], "}"}], ",", 
     SuperscriptBox["10", 
      RowBox[{"-", "6"}]], ",", "\[Infinity]"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.775541917935274*^9, 3.775541929914413*^9}, {
  3.775542017052538*^9, 3.775542060069221*^9}},
 CellLabel->
  "In[129]:=",ExpressionUUID->"78afef13-9b00-46d4-bff7-82c4b7772337"],

