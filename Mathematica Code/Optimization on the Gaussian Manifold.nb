Cell[CellGroupData[{

Cell["Basis Transformation matrices", "Subchapter",
 CellChangeTimes->{{3.775657715061029*^9, 
  3.7756577200938396`*^9}},ExpressionUUID->"4dd34723-4023-4bb5-b58e-\
dc2c9240746f"],

Cell[TextData[{
 "We provide basis transformation matrices for transformations between four \
bases (note that ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["b", "i"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "79863c2d-ce68-4496-b84f-9f8f7d9c0ecd"],
 " are the respective annihilation operators):\n",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["q", "1"], ",", 
     SubscriptBox["p", "1"], ",", 
     SubscriptBox["q", "2"], ",", 
     SubscriptBox["p", "2"], ",", "..."}], ")"}], " "}]], "PrintUsage",
  CellChangeTimes->{3.775657675716322*^9},ExpressionUUID->
  "6f32ca9a-eabc-4cb4-803a-639f85770ac3"],
 "\t",
 Cell[BoxData[
  RowBox[{"(", 
   RowBox[{
    SubscriptBox["q", "1"], ",", 
    SubscriptBox["q", "2"], ",", "...", ",", 
    SubscriptBox["p", "1"], ",", 
    SubscriptBox["p", "2"], ",", "..."}], ")"}]], "PrintUsage",
  CellChangeTimes->{3.7756576793864727`*^9},ExpressionUUID->
  "4e5614d4-6a80-4294-908e-1d806d602204"],
 "\t",
 Cell[BoxData[
  RowBox[{"(", 
   RowBox[{
    SubscriptBox["a", "1"], ",", 
    SubscriptBox["b", "1"], ",", 
    SubscriptBox["a", "2"], ",", 
    SubscriptBox["b", "2"], ",", "..."}], ")"}]], "PrintUsage",
  CellChangeTimes->{3.775657685836769*^9},ExpressionUUID->
  "829b2d95-528f-4fd6-bbf7-389006fa6098"],
 "\t",
 Cell[BoxData[
  RowBox[{"(", 
   RowBox[{
    SubscriptBox["a", "1"], ",", 
    SubscriptBox["a", "2"], ",", "...", ",", 
    SubscriptBox["b", "1"], ",", 
    SubscriptBox["b", "2"], ",", "..."}], ")"}]], "PrintUsage",
  CellChangeTimes->{3.775657682271524*^9},ExpressionUUID->
  "62168ce4-b9e9-41fd-a26c-bc92d25bdb8c"],
 "\n\nwhere we have defined:\t\t",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["a", "i"], "=", 
   RowBox[{
    FractionBox["1", 
     SqrtBox["2"]], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["q", "i"], "+", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["p", "i"]}]}], ")"}]}]}]],
  CellChangeTimes->{3.775658024349708*^9},ExpressionUUID->
  "1cd4fc5e-4d6a-423f-9e42-95f4bfb9cd89"],
 "  and   ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["b", "i"], "=", 
   RowBox[{
    FractionBox["1", 
     SqrtBox["2"]], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["q", "i"], "-", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["p", "i"]}]}], ")"}]}]}]],
  CellChangeTimes->{3.775658024349708*^9},ExpressionUUID->
  "fc131a19-a473-490c-9786-4acb0222c7bd"],
 " "
}], "Text",
 CellChangeTimes->{{3.775657866858886*^9, 3.775657900048665*^9}, {
