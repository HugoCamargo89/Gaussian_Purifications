          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "n"}], "}"}]}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"val", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
