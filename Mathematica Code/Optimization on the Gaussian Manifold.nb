Cell[TextData[StyleBox["0. Initialize definitions: Functions to generate the \
initial covariance matrices for a lattice free scalar field theory",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Subsubsection",
 CellChangeTimes->{{3.775541412824267*^9, 3.7755416727390947`*^9}, {
  3.7755421075764008`*^9, 3.775542108507226*^9}, {3.77572309702594*^9, 
  3.775723107081567*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"422cc050-0ce6-457f-9713-a160ea40d791"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FourierMatrixElement", "[", "N_", "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"k", ",", "x"}], "}"}], ",", 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"I", " ", "2", " ", "Pi", " ", "x", " ", 
        RowBox[{"k", " ", "/", "N"}]}], "]"}], "/", 
      RowBox[{"Sqrt", "[", "N", "]"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DispersionRelation", "[", 
    RowBox[{"m_", ",", "N_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", "k", "}"}], ",", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"m", "^", "2"}], "+", 
       RowBox[{
        RowBox[{"4", "/", 
         RowBox[{"l", "^", "2"}]}], "  ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Sin", "[", 
           RowBox[{"Pi", " ", 
            RowBox[{"k", "/", "N"}]}], "]"}], ")"}], "^", "2"}]}]}], "]"}]}], 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.775303771446594*^9, 3.775303847540307*^9}, {
   3.7753044069466248`*^9, 3.77530442747785*^9}, {3.775304555993222*^9, 
   3.7753045582432203`*^9}, 3.775541349319888*^9},
 CellLabel->
  "In[659]:=",ExpressionUUID->"fd4be0d6-208a-4b8c-bcbd-b30ef9f076cd"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"FT", "[", "n_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"f", ",", "pos", ",", "val"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"f", "=", 
       RowBox[{"FourierMatrixElement", "[", "n", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"pos", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "k"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "n"}], "}"}]}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"val", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"f", "[", 
            RowBox[{"k", ",", "x"}], "]"}], "//", "N"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "n"}], "}"}]}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"SparseArray", "[", 
