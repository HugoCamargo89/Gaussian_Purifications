Cell[CellGroupData[{

Cell[TextData[StyleBox["0. Initialize definitions: Functions to generate the \
initial covariance matrices for the XY model",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Subsubsection",
 CellChangeTimes->{{3.775541412824267*^9, 3.7755416727390947`*^9}, {
  3.7755421075764008`*^9, 3.775542108507226*^9}, {3.77572309702594*^9, 
  3.775723107081567*^9}, {3.7760828975986233`*^9, 3.7760829005628567`*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"aae3d3bf-7741-4e64-bcbd-dee277af3536"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]", "[", "k_", "]"}], "=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["h", "2"], "+", 
     RowBox[{"2", "h", " ", "J", " ", 
      RowBox[{"Cos", "[", "k", "]"}]}], "+", 
     SuperscriptBox["J", "2"], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["\[Gamma]", "2"], "-", "1"}], ")"}], 
      SuperscriptBox["J", "2"], 
      SuperscriptBox[
       RowBox[{"Sin", "[", "k", "]"}], "2"]}]}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"a", "[", "k_", "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "J"}], " ", 
     RowBox[{"Cos", "[", "k", "]"}]}], "-", "h"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"b", "[", "k_", "]"}], "=", 
   RowBox[{"\[Gamma]", " ", "J", " ", 
    RowBox[{"Sin", "[", "k", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", "k_", "]"}], "=", 
   FractionBox[
    RowBox[{
     RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
     RowBox[{"a", "[", "k", "]"}]}], 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"\[Epsilon]", "[", "k", "]"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
        RowBox[{"a", "[", "k", "]"}]}], ")"}]}]]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"v", "[", "k_", "]"}], "=", 
   FractionBox[
    RowBox[{"\[ImaginaryI]", " ", 
     RowBox[{"b", "[", "k", "]"}]}], 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"\[Epsilon]", "[", "k", "]"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
        RowBox[{"a", "[", "k", "]"}]}], ")"}]}]]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Sf", "[", "xx_", "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       RowBox[{"1", "+", "xx"}], "2"]}], 
     RowBox[{"Log", "[", 
      RowBox[{"2", ",", 
       FractionBox[
        RowBox[{"1", "+", "xx"}], "2"]}], "]"}]}], "-", 
    RowBox[{
     FractionBox[
      RowBox[{"1", "-", "xx"}], "2"], 
     RowBox[{"Log", "[", 
      RowBox[{"2", ",", 
       FractionBox[
        RowBox[{"1", "-", "xx"}], "2"]}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]min", "[", 
    RowBox[{"J_", ",", "h_", ",", "\[Gamma]_"}], "]"}], ":=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["J", "2"], "+", 
       FractionBox[
        SuperscriptBox["h", "2"], 
        RowBox[{
         SuperscriptBox["\[Gamma]", "2"], "-", "1"}]]}], ")"}]}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]max", "[", 
    RowBox[{"J_", ",", "h_", ",", "\[Gamma]_"}], "]"}], ":=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["J", "2"], "+", 
     RowBox[{"2", "h", " ", "J"}], "+", 
     SuperscriptBox["h", "2"]}]]}], ";"}]}], "Input",
 CellChangeTimes->{{3.775922842526188*^9, 3.775922843510675*^9}},
 CellLabel->"In[57]:=",ExpressionUUID->"97f51cd7-0b58-464d-abe7-b644b9e67237"]
}, Closed]],

Cell[CellGroupData[{

Cell[TextData[StyleBox["1. Generate the initial mixed state covariance matrix",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Subsubsection",
 CellChangeTimes->{{3.775541412824267*^9, 3.7755416727390947`*^9}, {
  3.7755421075764008`*^9, 3.775542108507226*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"58edae5e-bbcb-400f-b463-3efa3d0528ff"],

Cell[TextData[{
 StyleBox["Note: ",
  FontWeight->"Bold"],
 "The functions used to define the initial mixed state covariance matrix \
below are beyond the scope of the GaussianOptimization.m package and are not \
included in it.\nIn this case we are generating a covariance matrix for a \
system with a total of 10 lattice sites and partitioning it into two adjacent \
subsystems containing 2 lattice sites each."
}], "Text",
 CellChangeTimes->{
  3.775542113593465*^9, {3.775883895105447*^9, 3.775883895628008*^9}, {
   3.775922699733684*^9, 3.775922704503661*^9}, {3.775922780015649*^9, 
   3.7759228157525263`*^9}, {3.775923038064014*^9, 
   3.775923039428996*^9}},ExpressionUUID->"4812c204-c166-47e8-923b-\
3f4771bd1739"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"NN", "=", "10"}], ";", 
  RowBox[{"J", "=", "1"}], ";", 
  RowBox[{"h", "=", ".5"}], ";", 
  RowBox[{"\[Gamma]", "=", ".5"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", "0", "]"}], "=", "0"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "0", "]"}], "=", "\[ImaginaryI]"}], ";", 
  RowBox[{
   RowBox[{"u", "[", "\[Pi]", "]"}], "=", "1"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "\[Pi]", "]"}], "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]block", "=", 
   RowBox[{
    FractionBox["1", "NN"], 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"Abs", "[", 
              RowBox[{"v", "[", 
               RowBox[{"2", "\[Pi]", " ", 
                RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"], "-", 
            SuperscriptBox[
             RowBox[{"Abs", "[", 
              RowBox[{"u", "[", 
               RowBox[{"2", "\[Pi]", " ", 
                RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"]}], ")"}], 
          RowBox[{"Cos", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}], "+", 
         RowBox[{"2", 
          RowBox[{"Im", "[", 
           RowBox[{
            RowBox[{"v", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}], 
            RowBox[{"u", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}]}], "]"}], 
          RowBox[{"Sin", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "0", ",", 
          RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", "NN"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"l", ",", "1", ",", "NN"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalOmega]\[CapitalOmega]", "=", 
    RowBox[{"ArrayFlatten", "[", 
     RowBox[{"(", "\[NoBreak]", GridBox[{
        {"0", "\[CapitalOmega]block"},
        {
         RowBox[{"-", 
          RowBox[{"Transpose", "[", "\[CapitalOmega]block", "]"}]}], "0"}
       }], "\[NoBreak]", ")"}], "]"}]}], ";", 
   RowBox[{"JJ", "=", "\[CapitalOmega]\[CapitalOmega]"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dim", "=", "10"}], ";", " ", 
  RowBox[{"dim1", "=", "2"}], ";", " ", 
  RowBox[{"dim2", "=", "2"}], ";", " ", 
  RowBox[{"d", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"indices1", "=", 
   RowBox[{
    RowBox[{"Thread", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{"x", ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "dim1"}], "}"}]}], "]"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"x", "+", "dim"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "dim1"}], "}"}]}], "]"}]}], "}"}], "]"}], "//", 
    RowBox[{
     RowBox[{"Flatten", "[", 
      RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"indices2", "=", 
   RowBox[{
    RowBox[{"Thread", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"dim1", "+", "d", "+", "x"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "dim2"}], "}"}]}], "]"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"dim1", "+", "d", "+", "x", "+", "dim"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "dim2"}], "}"}]}], "]"}]}], "}"}], "]"}], "//", 
    RowBox[{
     RowBox[{"Flatten", "[", 
      RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"indices", "=", 
    RowBox[{"Join", "[", 
     RowBox[{"indices1", ",", "indices2"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Tran", "=", 
   RowBox[{"GOqpqpFROMqqpp", "[", "dim", "]"}]}], ";", " ", 
  RowBox[{"\[CapitalOmega]\[CapitalOmega]qpqp", "=", 
   RowBox[{"Tran", ".", "\[CapitalOmega]\[CapitalOmega]", ".", 
    RowBox[{"Transpose", "[", "Tran", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]0", "=", 
   RowBox[{"\[CapitalOmega]\[CapitalOmega]qpqp", "[", 
    RowBox[{"[", 
     RowBox[{"indices", ",", "indices"}], "]"}], "]"}]}], ";", " ", 
  RowBox[{"\[CapitalOmega]0", "//", "MatrixForm"}]}]}], "Input",
 CellChangeTimes->{{3.775922971943037*^9, 3.7759229870338697`*^9}, {
  3.775923033527286*^9, 3.7759230341155376`*^9}, {3.775923066880962*^9, 
  3.775923069102537*^9}, {3.77592371297513*^9, 3.775923776118202*^9}},
 CellLabel->"In[65]:=",ExpressionUUID->"505f6d93-332c-418b-96a4-ce54ce7f2996"]
}, Closed]],

