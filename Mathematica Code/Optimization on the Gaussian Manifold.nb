Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"CM", "[", 
    RowBox[{"N_", ",", "m_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"F", ",", "D", ",", "invD", ",", "G", ",", "Tr"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"F", "=", 
       RowBox[{"FT", "[", "N", "]"}]}], ";", 
      RowBox[{"D", "=", 
       RowBox[{"Diag", "[", 
        RowBox[{"m", ",", "N", ",", "l"}], "]"}]}], ";", " ", 
      RowBox[{"invD", "=", 
       RowBox[{"invDiag", "[", 
        RowBox[{"m", ",", "N", ",", "l"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"G", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"ArrayFlatten", "[", 
          RowBox[{"(", "\[NoBreak]", GridBox[{
             {
              RowBox[{"F", ".", "D", ".", 
               RowBox[{"ConjugateTranspose", "[", "F", "]"}]}], "0"},
             {"0", 
              RowBox[{"F", ".", "invD", ".", 
               RowBox[{"ConjugateTranspose", "[", "F", "]"}]}]}
            }], "\[NoBreak]", ")"}], "]"}], "//", "Chop"}], "//", 
        "SparseArray"}]}]}]}], "]"}]}], ";"}]], "Input",
