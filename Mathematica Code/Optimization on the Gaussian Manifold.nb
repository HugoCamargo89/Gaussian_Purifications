       RowBox[{"FourierMatrixElement", "[", "n", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"pos", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "k"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "n"}], "}"}]}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"val", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"f", "[", 
            RowBox[{"k", ",", "x"}], "]"}], "//", "N"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "n"}], "}"}]}], "]"}], "//", 
        RowBox[{
