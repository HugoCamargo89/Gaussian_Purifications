a5d96025045f"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   SubscriptBox["tol", "F"], TraditionalForm]],
  FontWeight->"Bold",
  FontSlant->"Italic",ExpressionUUID->
  "7b546bd2-0098-41f3-a704-fdad7dc9b2f3"],
 " ",
 StyleBox["-- Tolerance on the function values ",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox["(stopping criterion: ",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["F", 
        RowBox[{"n", "+", "1"}]], "-", 
       SubscriptBox["F", "n"]}], ")"}], "/", 
     SubscriptBox["F", 
      RowBox[{"n", "+", "1"}]]}], "<", 
    SubscriptBox["tol", "F"]}], TraditionalForm]],ExpressionUUID->
  "95c2f2ca-a21f-4d91-8d7d-02b6ab6f4d2b"],
 StyleBox[")",
  FontSlant->"Italic"]
}], "Text",
