Cell[TextData[StyleBox["0. Initialize definitions: Functions to generate the \
initial covariance matrices for a lattice free scalar field theory",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Subsubsection",
 CellChangeTimes->{{3.775541412824267*^9, 3.7755416727390947`*^9}, {
  3.7755421075764008`*^9, 3.775542108507226*^9}, {3.77572309702594*^9, 
  3.775723107081567*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"aae3d3bf-7741-4e64-bcbd-dee277af3536"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]", "[", "k_", "]"}], "=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["h", "2"], "+", 
     RowBox[{"2", "h", " ", "J", " ", 
      RowBox[{"Cos", "[", "k", "]"}]}], "+", 
     SuperscriptBox["J", "2"], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["\[Gamma]", "2"], "-", "1"}], ")"}], 
      SuperscriptBox["J", "2"], 
      SuperscriptBox[
       RowBox[{"Sin", "[", "k", "]"}], "2"]}]}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"a", "[", "k_", "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "J"}], " ", 
     RowBox[{"Cos", "[", "k", "]"}]}], "-", "h"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"b", "[", "k_", "]"}], "=", 
   RowBox[{"\[Gamma]", " ", "J", " ", 
    RowBox[{"Sin", "[", "k", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", "k_", "]"}], "=", 
   FractionBox[
    RowBox[{
     RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
     RowBox[{"a", "[", "k", "]"}]}], 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"\[Epsilon]", "[", "k", "]"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
        RowBox[{"a", "[", "k", "]"}]}], ")"}]}]]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"v", "[", "k_", "]"}], "=", 
   FractionBox[
    RowBox[{"\[ImaginaryI]", " ", 
     RowBox[{"b", "[", "k", "]"}]}], 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"\[Epsilon]", "[", "k", "]"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
        RowBox[{"a", "[", "k", "]"}]}], ")"}]}]]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Sf", "[", "xx_", "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       RowBox[{"1", "+", "xx"}], "2"]}], 
     RowBox[{"Log", "[", 
      RowBox[{"2", ",", 
       FractionBox[
        RowBox[{"1", "+", "xx"}], "2"]}], "]"}]}], "-", 
    RowBox[{
     FractionBox[
      RowBox[{"1", "-", "xx"}], "2"], 
     RowBox[{"Log", "[", 
      RowBox[{"2", ",", 
       FractionBox[
        RowBox[{"1", "-", "xx"}], "2"]}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]min", "[", 
    RowBox[{"J_", ",", "h_", ",", "\[Gamma]_"}], "]"}], ":=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["J", "2"], "+", 
       FractionBox[
        SuperscriptBox["h", "2"], 
        RowBox[{
         SuperscriptBox["\[Gamma]", "2"], "-", "1"}]]}], ")"}]}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]max", "[", 
    RowBox[{"J_", ",", "h_", ",", "\[Gamma]_"}], "]"}], ":=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["J", "2"], "+", 
     RowBox[{"2", "h", " ", "J"}], "+", 
