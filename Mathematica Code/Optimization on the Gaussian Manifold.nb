Cell[TextData[{
 StyleBox["Note: ",
  FontWeight->"Bold"],
 "The functions used to define the initial mixed state covariance matrix \
below are beyond the scope of the GaussianOptimization.m package and are not \
included in it.\nIn this case we are generating a covariance matrix for a \
system with a total of 10 lattice sites and partitioning it into two adjacent \
subsystems containing 2 lattice sites each."
}], "Text",
 CellChangeTimes->{
  3.775542113593465*^9, {3.775883895105447*^9, 3.775883895628008*^9}, {
   3.775922699733684*^9, 3.775922704503661*^9}, {3.775922780015649*^9, 
   3.7759228157525263`*^9}, {3.775923038064014*^9, 
   3.775923039428996*^9}},ExpressionUUID->"4812c204-c166-47e8-923b-\
3f4771bd1739"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"NN", "=", "10"}], ";", 
  RowBox[{"J", "=", "1"}], ";", 
  RowBox[{"h", "=", ".5"}], ";", 
  RowBox[{"\[Gamma]", "=", ".5"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", "0", "]"}], "=", "0"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "0", "]"}], "=", "\[ImaginaryI]"}], ";", 
  RowBox[{
   RowBox[{"u", "[", "\[Pi]", "]"}], "=", "1"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "\[Pi]", "]"}], "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]block", "=", 
   RowBox[{
    FractionBox["1", "NN"], 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"Abs", "[", 
              RowBox[{"v", "[", 
               RowBox[{"2", "\[Pi]", " ", 
                RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"], "-", 
            SuperscriptBox[
             RowBox[{"Abs", "[", 
              RowBox[{"u", "[", 
               RowBox[{"2", "\[Pi]", " ", 
                RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"]}], ")"}], 
          RowBox[{"Cos", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}], "+", 
         RowBox[{"2", 
          RowBox[{"Im", "[", 
           RowBox[{
            RowBox[{"v", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}], 
            RowBox[{"u", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}]}], "]"}], 
          RowBox[{"Sin", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "0", ",", 
          RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", "NN"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"l", ",", "1", ",", "NN"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalOmega]\[CapitalOmega]", "=", 
    RowBox[{"ArrayFlatten", "[", 
     RowBox[{"(", "\[NoBreak]", GridBox[{
        {"0", "\[CapitalOmega]block"},
        {
         RowBox[{"-", 
          RowBox[{"Transpose", "[", "\[CapitalOmega]block", "]"}]}], "0"}
       }], "\[NoBreak]", ")"}], "]"}]}], ";", 
   RowBox[{"JJ", "=", "\[CapitalOmega]\[CapitalOmega]"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dim", "=", "10"}], ";", " ", 
  RowBox[{"dim1", "=", "2"}], ";", " ", 
  RowBox[{"dim2", "=", "2"}], ";", " ", 
  RowBox[{"d", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"indices1", "=", 
   RowBox[{
    RowBox[{"Thread", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{"x", ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "dim1"}], "}"}]}], "]"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"x", "+", "dim"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "dim1"}], "}"}]}], "]"}]}], "}"}], "]"}], "//", 
    RowBox[{
     RowBox[{"Flatten", "[", 
      RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"indices2", "=", 
   RowBox[{
    RowBox[{"Thread", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"dim1", "+", "d", "+", "x"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "dim2"}], "}"}]}], "]"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"dim1", "+", "d", "+", "x", "+", "dim"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "dim2"}], "}"}]}], "]"}]}], "}"}], "]"}], "//", 
    RowBox[{
     RowBox[{"Flatten", "[", 
      RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"indices", "=", 
    RowBox[{"Join", "[", 
     RowBox[{"indices1", ",", "indices2"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Tran", "=", 
   RowBox[{"GOqpqpFROMqqpp", "[", "dim", "]"}]}], ";", " ", 
  RowBox[{"\[CapitalOmega]\[CapitalOmega]qpqp", "=", 
   RowBox[{"Tran", ".", "\[CapitalOmega]\[CapitalOmega]", ".", 
    RowBox[{"Transpose", "[", "Tran", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]0", "=", 
   RowBox[{"\[CapitalOmega]\[CapitalOmega]qpqp", "[", 
    RowBox[{"[", 
     RowBox[{"indices", ",", "indices"}], "]"}], "]"}]}], ";", " ", 
  RowBox[{"\[CapitalOmega]0", "//", "MatrixForm"}]}]}], "Input",
