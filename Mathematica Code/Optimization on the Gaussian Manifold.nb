                RowBox[{"2", "\[Pi]", " ", 
                 RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"]}], "//", 
            "N"}], ")"}], 
          RowBox[{"Cos", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}], "+", 
         RowBox[{"2", 
          RowBox[{"Im", "[", 
           RowBox[{
            RowBox[{"v", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}], 
            RowBox[{"u", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}]}], "]"}], 
          RowBox[{"Sin", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}]}], ",", 
