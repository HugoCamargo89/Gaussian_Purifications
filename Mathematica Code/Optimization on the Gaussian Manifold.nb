Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FourierMatrixElement", "[", "N_", "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"k", ",", "x"}], "}"}], ",", 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"I", " ", "2", " ", "Pi", " ", "x", " ", 
        RowBox[{"k", " ", "/", "N"}]}], "]"}], "/", 
      RowBox[{"Sqrt", "[", "N", "]"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DispersionRelation", "[", 
    RowBox[{"m_", ",", "N_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", "k", "}"}], ",", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"m", "^", "2"}], "+", 
       RowBox[{
        RowBox[{"4", "/", 
         RowBox[{"l", "^", "2"}]}], "  ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Sin", "[", 
           RowBox[{"Pi", " ", 
            RowBox[{"k", "/", "N"}]}], "]"}], ")"}], "^", "2"}]}]}], "]"}]}], 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.775303771446594*^9, 3.775303847540307*^9}, {
   3.7753044069466248`*^9, 3.77530442747785*^9}, {3.775304555993222*^9, 
   3.7753045582432203`*^9}, 3.775541349319888*^9},
 CellLabel->"In[4]:=",ExpressionUUID->"f9bf936a-418c-44aa-a4a6-1237d5fd2152"],

Cell[BoxData[
