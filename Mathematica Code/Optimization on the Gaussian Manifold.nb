  RowBox[{
   RowBox[{"u", "[", "\[Pi]", "]"}], "=", "1"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "\[Pi]", "]"}], "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]block", "=", 
   RowBox[{
    FractionBox["1", "NN"], 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             SuperscriptBox[
