Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"KGft", "[", 
    RowBox[{"{", 
     RowBox[{"NN_", ",", "\[Delta]_", ",", "m_", ",", "\[Mu]_"}], "}"}], 
    "]"}], ":=", 
   RowBox[{
    RowBox[{"KGft", "[", 
     RowBox[{"{", 
      RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "\[Mu]"}], "}"}], "]"}], 
    "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Fourier", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          FractionBox[
           SqrtBox[
            FractionBox[
             RowBox[{
              SuperscriptBox["m", "2"], "+", 
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{
                 FractionBox["2", "\[Delta]"], 
                 RowBox[{"Sin", "[", 
                  FractionBox[
                   RowBox[{"\[Pi]", " ", "k"}], "NN"], "]"}]}], ")"}], "2"]}],
              "NN"]], "\[Mu]"], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], "//", "Re"}], 
      ",", 
      RowBox[{
       RowBox[{"Fourier", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"\[Mu]", "/", 
           SqrtBox[
            RowBox[{"NN", 
             RowBox[{"(", 
              RowBox[{
               SuperscriptBox["m", "2"], "+", 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{
                  FractionBox["2", "\[Delta]"], 
                  RowBox[{"Sin", "[", 
                   FractionBox[
                    RowBox[{"\[Pi]", " ", "k"}], "NN"], "]"}]}], ")"}], 
                "2"]}], ")"}]}]]}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], "//", "Re"}]}],
      "}"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"KGb", "[", "var_", "]"}], ":=", 
   RowBox[{
    RowBox[{"KGb", "[", "var", "]"}], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"ToeplitzMatrix", "[", 
       RowBox[{
        RowBox[{"KGft", "[", "var", "]"}], "\[LeftDoubleBracket]", "1", 
        "\[RightDoubleBracket]"}], "]"}], ",", 
      RowBox[{"ToeplitzMatrix", "[", 
       RowBox[{
        RowBox[{"KGft", "[", "var", "]"}], "\[LeftDoubleBracket]", "2", 
        "\[RightDoubleBracket]"}], "]"}]}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"KGcm", "[", "var_", "]"}], ":=", 
    RowBox[{
     RowBox[{"KGcm", "[", "var", "]"}], "=", 
     RowBox[{"ArrayFlatten", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"KGb", "[", "var", "]"}], "\[LeftDoubleBracket]", "1", 
           "\[RightDoubleBracket]"}], ",", "0."}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0.", ",", 
          RowBox[{
           RowBox[{"KGb", "[", "var", "]"}], "\[LeftDoubleBracket]", "2", 
           "\[RightDoubleBracket]"}]}], "}"}]}], "}"}], "]"}]}]}], ";"}], 
  RowBox[{"(*", " ", 
   RowBox[{"in", " ", "qqpp", " ", "basis"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"KGcmRes", "[", 
   RowBox[{"NN\[Delta]m\[Mu]_", ",", 
    RowBox[{"{", 
     RowBox[{"dimA_", ",", "dimB_", ",", "d_"}], "}"}]}], "]"}], ":=", 
  RowBox[{
   RowBox[{"KGcmRes", "[", 
    RowBox[{"NN\[Delta]m\[Mu]", ",", 
     RowBox[{"{", 
      RowBox[{"dimA", ",", "dimB", ",", "d"}], "}"}]}], "]"}], "=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "ind", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ind", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{"i", ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "dimA"}], "}"}]}], "]"}], ",", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"i", "+", "dimA", "+", "d"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "dimB"}], "}"}]}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"GOqpqpFROMqqpp", "[", 
        RowBox[{"dimA", "+", "dimB"}], "]"}], ".", 
       RowBox[{"ArrayFlatten", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"KGb", "[", "NN\[Delta]m\[Mu]", "]"}], 
             "\[LeftDoubleBracket]", 
             RowBox[{"1", ",", "ind", ",", "ind"}], "\[RightDoubleBracket]"}],
             ",", "0."}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0.", ",", 
            RowBox[{
             RowBox[{"KGb", "[", "NN\[Delta]m\[Mu]", "]"}], 
             "\[LeftDoubleBracket]", 
             RowBox[{"2", ",", "ind", ",", "ind"}], 
             "\[RightDoubleBracket]"}]}], "}"}]}], "}"}], "]"}], ".", 
       RowBox[{"Transpose", "[", 
        RowBox[{"GOqpqpFROMqqpp", "[", 
         RowBox[{"dimA", "+", "dimB"}], "]"}], "]"}]}]}]}], 
    "\[IndentingNewLine]", "]"}], 
   RowBox[{"(*", " ", 
    RowBox[{"in", " ", "qpqp", " ", "basis"}], " ", "*)"}]}]}]}], "Code",
