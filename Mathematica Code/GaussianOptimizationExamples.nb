Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", "0", "]"}], "=", "0"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "0", "]"}], "=", "\[ImaginaryI]"}], ";", 
  RowBox[{
   RowBox[{"u", "[", "\[Pi]", "]"}], "=", "1"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "\[Pi]", "]"}], "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]block", "=", 
   RowBox[{
    FractionBox["1", "NN"], 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             SuperscriptBox[
              RowBox[{"Abs", "[", 
               RowBox[{"v", "[", 
                RowBox[{"2", "\[Pi]", " ", 
                 RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"], "-", 
             SuperscriptBox[
              RowBox[{"Abs", "[", 
               RowBox[{"u", "[", 
                RowBox[{"2", "\[Pi]", " ", 
                 RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"]}], "//", 
            "N"}], ")"}], 
          RowBox[{"Cos", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}], "+", 
         RowBox[{"2", 
          RowBox[{"Im", "[", 
           RowBox[{
            RowBox[{"v", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}], 
            RowBox[{"u", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}]}], "]"}], 
          RowBox[{"Sin", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "0", ",", 
          RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", "NN"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"l", ",", "1", ",", "NN"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]\[CapitalOmega]", "=", 
   RowBox[{"ArrayFlatten", "[", 
    RowBox[{"(", "\[NoBreak]", GridBox[{
       {"0", "\[CapitalOmega]block"},
       {
        RowBox[{"-", 
         RowBox[{"Transpose", "[", "\[CapitalOmega]block", "]"}]}], "0"}
