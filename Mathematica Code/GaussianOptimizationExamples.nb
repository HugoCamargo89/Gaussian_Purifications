Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FourierMatrixElement", "[", "N_", "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"k", ",", "x"}], "}"}], ",", 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"I", " ", "2", " ", "Pi", " ", "x", " ", 
        RowBox[{"k", " ", "/", "N"}]}], "]"}], "/", 
      RowBox[{"Sqrt", "[", "N", "]"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DispersionRelation", "[", 
    RowBox[{"m_", ",", "N_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", "k", "}"}], ",", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"m", "^", "2"}], "+", 
       RowBox[{
        RowBox[{"4", "/", 
         RowBox[{"l", "^", "2"}]}], "  ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Sin", "[", 
           RowBox[{"Pi", " ", 
            RowBox[{"k", "/", "N"}]}], "]"}], ")"}], "^", "2"}]}]}], "]"}]}], 
    "]"}]}], ";"}]}], "Input",
