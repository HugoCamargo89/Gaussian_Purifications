Cell[TextData[{
 "We provide basis transformation matrices for transformations between four \
bases (note that ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["b", "i"], TraditionalForm]],ExpressionUUID->
  "62ceb368-0cb9-407a-86d2-55fa6a24ddbf"],
 " are the respective annihilation operators):\n",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["q", "1"], ",", 
     SubscriptBox["p", "1"], ",", 
     SubscriptBox["q", "2"], ",", 
     SubscriptBox["p", "2"], ",", "..."}], ")"}], " "}]], "PrintUsage",
  CellChangeTimes->{3.775657675716322*^9},ExpressionUUID->
  "38803c70-1652-42d7-8db3-91a9e3626c8c"],
 "\t",
 Cell[BoxData[
  RowBox[{"(", 
   RowBox[{
    SubscriptBox["q", "1"], ",", 
    SubscriptBox["q", "2"], ",", "...", ",", 
    SubscriptBox["p", "1"], ",", 
    SubscriptBox["p", "2"], ",", "..."}], ")"}]], "PrintUsage",
  CellChangeTimes->{3.7756576793864727`*^9},ExpressionUUID->
  "514cda29-3a6e-4162-b4f5-e4a41ba3e73f"],
 "\t",
 Cell[BoxData[
  RowBox[{"(", 
   RowBox[{
    SubscriptBox["a", "1"], ",", 
    SubscriptBox["b", "1"], ",", 
    SubscriptBox["a", "2"], ",", 
    SubscriptBox["b", "2"], ",", "..."}], ")"}]], "PrintUsage",
  CellChangeTimes->{3.775657685836769*^9},ExpressionUUID->
  "52c7d516-2fe2-42b2-888c-0e37ac08c6f6"],
 "\t",
 Cell[BoxData[
  RowBox[{"(", 
   RowBox[{
    SubscriptBox["a", "1"], ",", 
    SubscriptBox["a", "2"], ",", "...", ",", 
    SubscriptBox["b", "1"], ",", 
    SubscriptBox["b", "2"], ",", "..."}], ")"}]], "PrintUsage",
  CellChangeTimes->{3.775657682271524*^9},ExpressionUUID->
  "879a563e-e498-4b6d-804c-73c039c8b632"],
 "\n\nwhere we have defined:\t\t",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["a", "i"], "=", 
   RowBox[{
    FractionBox["1", 
     SqrtBox["2"]], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["q", "i"], "+", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["p", "i"]}]}], ")"}]}]}]],
  CellChangeTimes->{3.775658024349708*^9},ExpressionUUID->
  "8c48ece2-2418-4223-8539-cb0cd45bd857"],
 "  and   ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["b", "i"], "=", 
   RowBox[{
    FractionBox["1", 
     SqrtBox["2"]], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["q", "i"], "-", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["p", "i"]}]}], ")"}]}]}]],
