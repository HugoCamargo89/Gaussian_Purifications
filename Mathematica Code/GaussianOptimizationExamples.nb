Cell[CellGroupData[{

Cell[TextData[StyleBox["1. Generate the initial mixed state covariance matrix",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Subsubsection",
 CellChangeTimes->{{3.775541412824267*^9, 3.7755416727390947`*^9}, {
  3.7755421075764008`*^9, 3.775542108507226*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"316d1ed8-9c75-492e-9786-08dfa3ab3f9a"],

Cell[TextData[{
 StyleBox["Note: ",
  FontWeight->"Bold"],
 "The functions used to define the initial mixed state covariance matrix \
below are beyond the scope of the GaussianOptimization.m package and are not \
included in it.\nIn this case we are generating a covariance matrix for a \
system with a total of 10 lattice sites and partitioning it into two adjacent \
subsystems containing 2 lattice sites each."
}], "Text",
 CellChangeTimes->{
  3.775542113593465*^9, {3.775883895105447*^9, 3.775883895628008*^9}, {
   3.775922699733684*^9, 3.775922704503661*^9}, {3.775922780015649*^9, 
   3.7759228157525263`*^9}, {3.775923038064014*^9, 
   3.775923039428996*^9}},ExpressionUUID->"0cdb3e44-c19e-4cf3-a533-\
d29560d4a986"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", "0", "]"}], "=", "0"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "0", "]"}], "=", "\[ImaginaryI]"}], ";", 
  RowBox[{
   RowBox[{"u", "[", "\[Pi]", "]"}], "=", "1"}], ";", 
  RowBox[{
   RowBox[{"v", "[", "\[Pi]", "]"}], "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]block", "=", 
   RowBox[{
    FractionBox["1", "NN"], 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             SuperscriptBox[
              RowBox[{"Abs", "[", 
               RowBox[{"v", "[", 
                RowBox[{"2", "\[Pi]", " ", 
                 RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"], "-", 
             SuperscriptBox[
              RowBox[{"Abs", "[", 
               RowBox[{"u", "[", 
                RowBox[{"2", "\[Pi]", " ", 
                 RowBox[{"k", "/", "NN"}]}], "]"}], "]"}], "2"]}], "//", 
            "N"}], ")"}], 
          RowBox[{"Cos", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}], "+", 
         RowBox[{"2", 
          RowBox[{"Im", "[", 
           RowBox[{
            RowBox[{"v", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}], 
            RowBox[{"u", "[", 
             RowBox[{"2", "\[Pi]", " ", 
              RowBox[{"k", "/", "NN"}]}], "]"}]}], "]"}], 
          RowBox[{"Sin", "[", 
           RowBox[{"2", "\[Pi]", " ", "k", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"j", "-", "l"}], ")"}], "/", "NN"}]}], "]"}]}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "0", ",", 
          RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", "NN"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"l", ",", "1", ",", "NN"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
