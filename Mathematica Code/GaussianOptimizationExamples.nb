 Background->GrayLevel[
  0.85],ExpressionUUID->"06d2295d-b188-4c84-95dd-7da7c3be7983"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]", "[", "k_", "]"}], "=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["h", "2"], "+", 
     RowBox[{"2", "h", " ", "J", " ", 
      RowBox[{"Cos", "[", "k", "]"}]}], "+", 
     SuperscriptBox["J", "2"], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["\[Gamma]", "2"], "-", "1"}], ")"}], 
      SuperscriptBox["J", "2"], 
      SuperscriptBox[
       RowBox[{"Sin", "[", "k", "]"}], "2"]}]}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"a", "[", "k_", "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "J"}], " ", 
     RowBox[{"Cos", "[", "k", "]"}]}], "-", "h"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"b", "[", "k_", "]"}], "=", 
   RowBox[{"\[Gamma]", " ", "J", " ", 
    RowBox[{"Sin", "[", "k", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", "k_", "]"}], "=", 
   FractionBox[
    RowBox[{
     RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
     RowBox[{"a", "[", "k", "]"}]}], 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"\[Epsilon]", "[", "k", "]"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
        RowBox[{"a", "[", "k", "]"}]}], ")"}]}]]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"v", "[", "k_", "]"}], "=", 
   FractionBox[
    RowBox[{"\[ImaginaryI]", " ", 
     RowBox[{"b", "[", "k", "]"}]}], 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"\[Epsilon]", "[", "k", "]"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
        RowBox[{"a", "[", "k", "]"}]}], ")"}]}]]]}], ";"}]}], "Input",
