      RowBox[{"dim", "=", 
       RowBox[{"Length", "[", "bigCM", "]"}]}], ";", " ", 
      RowBox[{"halfdim", "=", 
       RowBox[{"dim", "/", "2"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"indices1", "=", 
       RowBox[{
        RowBox[{"Thread", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Table", "[", 
            RowBox[{"x", ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "dim1"}], "}"}]}], "]"}], ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"x", "+", "halfdim"}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "dim1"}], "}"}]}], "]"}]}], "}"}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"indices2", "=", 
       RowBox[{
        RowBox[{"Thread", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"dim1", "+", "d", "+", "x"}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "dim2"}], "}"}]}], "]"}], ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"dim1", "+", "d", "+", "x", "+", "halfdim"}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "dim2"}], "}"}]}], "]"}]}], "}"}], "]"}], "//", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}]}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"indices", "=", 
       RowBox[{"Join", "[", 
        RowBox[{"indices1", ",", "indices2"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"partialCM", "=", 
       RowBox[{"bigCM", "[", 
        RowBox[{"[", 
         RowBox[{"indices", ",", "indices"}], "]"}], "]"}]}]}]}], "]"}]}], 
  ";"}]], "Input",
