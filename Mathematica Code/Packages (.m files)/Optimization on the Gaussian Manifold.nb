Cell[CellGroupData[{

Cell["\<\
Example of the setup and execution for a typical optimization\
\>", "Subsubsection",
 CellChangeTimes->{{3.7755413578065434`*^9, 3.775541373881979*^9}},
 FontColor->GrayLevel[
  0],ExpressionUUID->"a17bcca1-74a2-4611-877c-08436d774f2e"],

Cell[TextData[StyleBox["1. Generate the initial mixed state covariance matrix",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.775541412824267*^9, 3.7755416727390947`*^9}, {
  3.7755421075764008`*^9, 3.775542108507226*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"03f1d981-3b56-4d87-be96-a52c3ba5853b"],

Cell[TextData[{
 StyleBox["Note: ",
  FontWeight->"Bold"],
 "The functions used to define the initial mixed state covariance matrix \
below are beyond the scope of the GaussianOptimization.m package and are not \
included in it.\nIn the example below, the mixed state is obtained for a \
system with total number of lattice sites 100 with 3 sites per subsystem in \
the initial system, mass term 0.1, lattice spacing 1, \nand no lattice sites \
separating the subsystems."
}], "Text",
 CellChangeTimes->{
  3.775542113593465*^9},ExpressionUUID->"6e98a748-a09b-4fc1-acf8-\
2cd6999c4643"],

Cell[BoxData[
 RowBox[{
  RowBox[{"CM0", "=", 
   RowBox[{"partialCM", "[", 
    RowBox[{"100", ",", ".1", ",", "1", ",", "0", ",", "3", ",", "3"}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7755414050121*^9, 3.775541406295203*^9}, {
  3.7755415228690567`*^9, 3.7755415240290537`*^9}, {3.77554159035139*^9, 
  3.775541608268277*^9}, {3.775541676390519*^9, 3.7755416774527903`*^9}},
 CellLabel->
  "In[169]:=",ExpressionUUID->"6f7b072c-4ed2-49fb-a471-de1cb22c64b5"],

Cell[TextData[StyleBox["2. Generate the purified state complex structure (in \
this case for a minimal purification)",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.775541691548297*^9, 3.7755417065181093`*^9}, {
  3.775541828555531*^9, 3.775541831328326*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"60cb2935-0c33-442d-bcb5-7e612bf3b320"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"rlist", ",", "MTra"}], "}"}], "=", 
   RowBox[{"GOExtractStdForm", "[", "CM0", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"J0", "=", 
   RowBox[{"GOConstructPurificationBos", "[", 
    RowBox[{"rlist", ",", 
     RowBox[{"{", 
      RowBox[{"6", ",", "6"}], "}"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.775541682190981*^9, 3.77554177782376*^9}},
 CellLabel->
  "In[173]:=",ExpressionUUID->"59182147-27a2-4d85-bd25-d97741096658"],

Cell[TextData[StyleBox["3. Generate a list of the starting points for the \
optimization (in this case 50 random transformations)",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.775541798902584*^9, 3.775541841904106*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"34ec6c9e-c65b-4201-8dc6-f5ea22aef4bb"],

Cell[BoxData[
 RowBox[{
  RowBox[{"M0List", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"ArrayFlatten", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Inverse", "[", "MTra", "]"}], ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", 
           RowBox[{"GOMranSp", "[", "12", "]"}]}], "}"}]}], "}"}], "]"}], "//",
       "SparseArray"}], ",", 
     RowBox[{"{", "50", "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.775541849659087*^9, 3.775541885906084*^9}},
 CellLabel->
  "In[175]:=",ExpressionUUID->"29f7bd6d-26a1-4eac-8e2d-c6cc013ea512"],

Cell[TextData[StyleBox["4. Define the function and gradient for the \
optimization (in this case choosing an equal distribution of deg. of freedom \
in the ancillary)",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.7755419413657427`*^9, 3.775541981278227*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"0da37d7e-0068-41a8-9d5f-461a1677db50"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Restriction", "=", 
   RowBox[{"GORestrictionAA", "[", 
    RowBox[{"{", 
     RowBox[{"3", ",", "3", ",", "3", ",", "3"}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"function", "=", 
   RowBox[{"GOEoPBos", "[", "Restriction", "]"}]}], ";", " ", 
  RowBox[{"gradient", "=", 
   RowBox[{"GOEoPgradBos", "[", "Restriction", "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.775541985290533*^9, 3.77554201468399*^9}},
 CellLabel->
  "In[176]:=",ExpressionUUID->"fd9d772e-e98f-4069-8000-5ed1fdbed01e"],

Cell[TextData[StyleBox["5. Execute the optimization",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.775541909217443*^9, 3.775541935738533*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"efef6185-8ab3-40c6-8c68-9db01f8c62e4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Result", "=", 
   RowBox[{"GOOptimize", "[", 
    RowBox[{"function", ",", "gradient", ",", "J0", ",", "M0List", ",", 
     RowBox[{"{", 
      RowBox[{"\"\<None\>\"", ",", "\"\<GOSpBasis\>\""}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"6", ",", "6"}], "}"}], ",", 
     SuperscriptBox["10", 
      RowBox[{"-", "6"}]], ",", "\[Infinity]"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.775541917935274*^9, 3.775541929914413*^9}, {
  3.775542017052538*^9, 3.775542060069221*^9}},
 CellLabel->
  "In[178]:=",ExpressionUUID->"78afef13-9b00-46d4-bff7-82c4b7772337"],

Cell[TextData[StyleBox["6. Evaluate the result",
 FontWeight->"Bold",
 FontSlant->"Italic"]], "Text",
 CellChangeTimes->{{3.77554209210205*^9, 3.775542096965893*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"d9f5051c-e394-4093-bac3-d3a883791d3e"],

