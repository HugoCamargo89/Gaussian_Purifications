          SqrtBox[
           RowBox[{"2", 
            RowBox[{"\[Epsilon]", "[", "k", "]"}], 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"\[Epsilon]", "[", "k", "]"}], "+", 
              RowBox[{"a", "[", "k", "]"}]}], ")"}]}]]]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"cor", "=", 
