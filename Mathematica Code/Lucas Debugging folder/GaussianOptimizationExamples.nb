        RowBox[{"v", "[", "0", "]"}], "=", "\[ImaginaryI]"}], ";", 
       RowBox[{
        RowBox[{"u", "[", "\[Pi]", "]"}], "=", "1"}], ";", 
       RowBox[{
        RowBox[{"v", "[", "\[Pi]", "]"}], "=", "0"}], ";", " ", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{
         "This", " ", "is", " ", "no", " ", "really", " ", "needed", " ", 
          "for", " ", "even", " ", "number", " ", "of", " ", "excitations"}], 
         ",", " ", 
         RowBox[{"but", " ", "would", " ", "be", " ", "relevant"}], ",", " ", 
         
         RowBox[{
         "if", " ", "we", " ", "were", " ", "to", " ", "construct", " ", 
          "eigenstates", " ", "with", " ", "odd", " ", "number", " ", "of", 
          " ", 
