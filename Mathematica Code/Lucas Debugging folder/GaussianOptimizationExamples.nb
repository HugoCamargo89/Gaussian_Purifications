            RowBox[{"Cos", "[", "k", "]"}]}], "+", 
           SuperscriptBox["J", "2"], "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              SuperscriptBox["\[Gamma]", "2"], "-", "1"}], ")"}], 
            SuperscriptBox["J", "2"], 
            SuperscriptBox[
             RowBox[{"Sin", "[", "k", "]"}], "2"]}]}]]}]}], ";", 
       "\[IndentingNewLine]", 
