Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"KGThft", "[", 
    RowBox[{"{", 
     RowBox[{"NN_", ",", "\[Delta]_", ",", "m_", ",", "\[Beta]_"}], "}"}], 
    "]"}], ":=", 
   RowBox[{
    RowBox[{"KGThft", "[", 
     RowBox[{"{", 
      RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "\[Beta]"}], "}"}], "]"}],
     "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Fourier", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Cosh", "[", 
            RowBox[{"2", 
             RowBox[{"\[Alpha]", "[", 
              RowBox[{
              "NN", ",", "\[Delta]", ",", "m", ",", "k", ",", "\[Beta]"}], 
              "]"}]}], "]"}], 
           FractionBox[
            RowBox[{"\[Omega]", "[", 
             RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}], 
            SqrtBox["NN"]]}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], "//", "Re"}], 
      ",", 
      RowBox[{
       RowBox[{"Fourier", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Cosh", "[", 
             RowBox[{"2", 
              RowBox[{"\[Alpha]", "[", 
               RowBox[{
               "NN", ",", "\[Delta]", ",", "m", ",", "k", ",", "\[Beta]"}], 
               "]"}]}], "]"}], "/", 
            RowBox[{"(", 
             RowBox[{
              SqrtBox["NN"], 
              RowBox[{"\[Omega]", "[", 
               RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]}], 
             ")"}]}], ")"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], "//", "Re"}]}],
      "}"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"KGThb", "[", "var_", "]"}], ":=", 
   RowBox[{
    RowBox[{"KGThb", "[", "var", "]"}], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"ToeplitzMatrix", "[", 
       RowBox[{
        RowBox[{"KGThft", "[", "var", "]"}], "\[LeftDoubleBracket]", "1", 
        "\[RightDoubleBracket]"}], "]"}], ",", 
      RowBox[{"ToeplitzMatrix", "[", 
       RowBox[{
        RowBox[{"KGThft", "[", "var", "]"}], "\[LeftDoubleBracket]", "2", 
        "\[RightDoubleBracket]"}], "]"}]}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"KGThcm", "[", "var_", "]"}], ":=", 
   RowBox[{
    RowBox[{"KGThcm", "[", "var", "]"}], "=", 
    RowBox[{"ArrayFlatten", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"KGThb", "[", "var", "]"}], "\[LeftDoubleBracket]", "1", 
          "\[RightDoubleBracket]"}], ",", "0."}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"0.", ",", 
         RowBox[{
          RowBox[{"KGThb", "[", "var", "]"}], "\[LeftDoubleBracket]", "2", 
          "\[RightDoubleBracket]"}]}], "}"}]}], "}"}], "]"}]}]}], ";", 
  RowBox[{"(*", " ", 
   RowBox[{"in", " ", "qqpp", " ", "basis"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"KGThcmRes", "[", 
    RowBox[{"NN\[Delta]m\[Beta]_", ",", 
     RowBox[{"{", 
      RowBox[{"dimA_", ",", "dimB_", ",", "d_"}], "}"}]}], "]"}], ":=", 
   RowBox[{
    RowBox[{"KGThcmRes", "[", 
     RowBox[{"NN\[Delta]m\[Beta]", ",", 
      RowBox[{"{", 
       RowBox[{"dimA", ",", "dimB", ",", "d"}], "}"}]}], "]"}], "=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "ind", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ind", "=", 
        RowBox[{"Join", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{"i", ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "dimA"}], "}"}]}], "]"}], ",", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"i", "+", "dimA", "+", "d"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "dimB"}], "}"}]}], "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"GOqpqpFROMqqpp", "[", 
         RowBox[{"dimA", "+", "dimB"}], "]"}], ".", 
        RowBox[{"ArrayFlatten", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"KGThb", "[", "NN\[Delta]m\[Beta]", "]"}], 
              "\[LeftDoubleBracket]", 
              RowBox[{"1", ",", "ind", ",", "ind"}], 
              "\[RightDoubleBracket]"}], ",", "0."}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"0.", ",", 
             RowBox[{
              RowBox[{"KGThb", "[", "NN\[Delta]m\[Beta]", "]"}], 
              "\[LeftDoubleBracket]", 
              RowBox[{"2", ",", "ind", ",", "ind"}], 
              "\[RightDoubleBracket]"}]}], "}"}]}], "}"}], "]"}], ".", 
        RowBox[{"Transpose", "[", 
         RowBox[{"GOqpqpFROMqqpp", "[", 
          RowBox[{"dimA", "+", "dimB"}], "]"}], "]"}]}]}]}], 
     "\[IndentingNewLine]", "]"}], 
    RowBox[{"(*", " ", 
     RowBox[{"in", " ", "qpqp", " ", "basis"}], " ", "*)"}]}]}]}]}], "Input",
