    RowBox[{"NN_", ",", "\[Delta]_", ",", "m_", ",", "k_", ",", "\[Beta]_"}], 
    "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"Log", "[", 
     RowBox[{"Coth", "[", 
      FractionBox[
       RowBox[{"\[Beta]", " ", 
