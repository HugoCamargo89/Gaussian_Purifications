 RowBox[{
  RowBox[{
   RowBox[{"\[Omega]", "[", 
    RowBox[{"NN_", ",", "\[Delta]_", ",", "m_", ",", "k_"}], "]"}], ":=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["m", "2"], "+", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        FractionBox["2", "\[Delta]"], 
        RowBox[{"Sin", "[", 
         FractionBox[
          RowBox[{"\[Pi]", " ", "k"}], "NN"], "]"}]}], ")"}], "2"]}]]}], 
  ";"}]], "Input",
