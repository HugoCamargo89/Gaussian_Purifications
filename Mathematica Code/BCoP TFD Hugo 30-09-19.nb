            RowBox[{"Sin", "[", 
             FractionBox[
              RowBox[{"t", " ", 
               RowBox[{"\[Omega]", "[", 
                RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]}], 
              "2"], "]"}], 
            RowBox[{"\[Omega]", "[", 
             RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]], 
