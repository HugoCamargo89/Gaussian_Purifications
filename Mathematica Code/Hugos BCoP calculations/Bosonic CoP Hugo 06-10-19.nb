Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Omega]", "[", 
    RowBox[{"NN_", ",", "\[Delta]_", ",", "m_", ",", "k_"}], "]"}], ":=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["m", "2"], "+", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        FractionBox["2", "\[Delta]"], 
        RowBox[{"Sin", "[", 
         FractionBox[
          RowBox[{"\[Pi]", " ", "k"}], "NN"], "]"}]}], ")"}], "2"]}]]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.778904831843864*^9, 3.778904875968898*^9}, {
  3.778906888152831*^9, 3.778906892631751*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"4c19f8ca-1f54-4b82-877c-5bf23c52ca13"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Alpha]", "[", 
    RowBox[{"NN_", ",", "\[Delta]_", ",", "m_", ",", "k_", ",", "\[Beta]_"}], 
    "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"Log", "[", 
     RowBox[{"Coth", "[", 
      FractionBox[
       RowBox[{"\[Beta]", " ", 
        RowBox[{"\[Omega]", "[", 
         RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]}], "4"], 
      "]"}], "]"}]}]}], ";"}]], "Input",
