Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MTFDft", "[", 
    RowBox[{"{", 
     RowBox[{"t_", ",", "NN_", ",", "\[Delta]_", ",", "m_"}], "}"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"MTFDft", "[", 
     RowBox[{"{", 
      RowBox[{"t", ",", "NN", ",", "\[Delta]", ",", "m"}], "}"}], "]"}], "=", 
    
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Fourier", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"Cos", "[", 
           FractionBox[
            RowBox[{"t", " ", 
             RowBox[{"\[Omega]", "[", 
              RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]}], 
            "2"], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], "//", "Re"}], 
      ",", 
      RowBox[{
       RowBox[{"Fourier", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"\[Omega]", "[", 
              RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]}], 
            RowBox[{"Sin", "[", 
             FractionBox[
              RowBox[{"t", " ", 
               RowBox[{"\[Omega]", "[", 
                RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]}], 
              "2"], "]"}]}], ")"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], "//", "Re"}], 
      ",", 
      RowBox[{
       RowBox[{"Fourier", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"(", 
           FractionBox[
            RowBox[{"Sin", "[", 
             FractionBox[
              RowBox[{"t", " ", 
               RowBox[{"\[Omega]", "[", 
                RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]}], 
              "2"], "]"}], 
            RowBox[{"\[Omega]", "[", 
             RowBox[{"NN", ",", "\[Delta]", ",", "m", ",", "k"}], "]"}]], 
           ")"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{"NN", "-", "1"}]}], "}"}]}], "]"}], "]"}], "//", "Re"}]}],
      "}"}]}]}], ";"}]], "Input",
